@model BDnew_class.Models.Class
@{
    ViewBag.Title = "Добавление пользователя";
}
    <form asp-action="create" asp-controller="home">
        <div class="form-group">
            <label asp-for="NameClass" class="control-label">Имя</label>
            <input type="text" asp-for="NameClass" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="ClassVid" class="control-label">Возраст</label>
            <input type="text" asp-for="ClassVid" class="form-control" />
        </div>
        <div class="form-group">
            <input type="submit" value="Отправить" class="btn btn-default" />
        </div>
        <div class="form-group">
            <label asp-for="ClassDescription" class="control-label">Имя</label>
            <input type="text" asp-for="ClassDescription" class="form-control" />
        </div>
        
    </form>






